def start_requests(self):
    '''
    Spider entrypoint.
    Request chaining starts from here.
    '''
    url: str = "https://farmaciasgi.com.mx/wp-content/themes/farmaciasGi/sucursales.json"

    yield scrapy.Request(
        url=url,
        callback=self.parse_contacts
    )

def parse(self, response):
    responseData = response.json()

    for row in responseData['features']:
        data = {
            'website': 'https://farmaciasgi.com.mx/wp-content/themes/farmaciasGi/sucursales.json',
            'ref': row['id'],
            'lat': float(row['geometry']['coordinates'][0]),
            'lon': float(row['geometry']['coordinates'][1]),
            'nombre': row['properties'],
            'estado': row['properties'],
            'municipio': row['properties'],
            'cp': int(row['properties']),
            'direccion': row['properties'],
        }

        yield GeojsonPointItem(**data)
